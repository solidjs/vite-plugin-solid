{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": [
      "vite/client",
      "@testing-library/jest-dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./pages/*"],
      "@@/*": ["./assets/*"]
    }
  }
}
